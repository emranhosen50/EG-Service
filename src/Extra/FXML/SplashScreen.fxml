<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="850.0" prefWidth="1600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.SplashScreenController">
   <children>
      <MediaView fx:id="mv" fitHeight="850.0" fitWidth="1600.0" onMouseDragged="#mouseDrag" onMousePressed="#mousePressed" />
      <Pane fx:id="bar" onMouseDragged="#mouseDrag" onMousePressed="#mousePressed" prefHeight="40.0" prefWidth="1600.0" style="-fx-background-color: #ffffff;">
         <children>
            <Circle fx:id="close" fill="RED" layoutX="1565.0" layoutY="20.0" onMouseClicked="#MouseClick" radius="9.0" stroke="WHITE" strokeType="INSIDE" strokeWidth="0.0" />
            <Circle fx:id="minimize" fill="#08e900" layoutX="1528.0" layoutY="20.0" onMouseClicked="#MouseClick" radius="9.0" stroke="WHITE" strokeType="INSIDE" strokeWidth="0.0" />
         </children>
      </Pane>
      <JFXButton fx:id="RefreshButton" layoutX="691.0" layoutY="733.0" onAction="#ActionEvent" prefHeight="13.0" prefWidth="235.0" style="-fx-background-color: #D9D8DA;" text="Start" textFill="#808080">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </JFXButton>
      <ProgressIndicator fx:id="loading" layoutX="774.0" layoutY="638.0" prefHeight="68.0" prefWidth="54.0" />
   </children>
</AnchorPane>
